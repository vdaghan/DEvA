#include "deva_version.h"

std::string getDEvAVersion() {
	return "@DEvA_VERSION@";
}

unsigned getDEvAVersionMajor() {
	return @DEvA_VERSION_MAJOR@;
}

unsigned getDEvAVersionMinor() {
	return @DEvA_VERSION_MINOR@ +0;
}

unsigned getDEvAVersionPatch() {
	return @DEvA_VERSION_PATCH@ +0;
}

unsigned getDEvAVersionTweak() {
	return @DEvA_VERSION_TWEAK@ +0;
}
